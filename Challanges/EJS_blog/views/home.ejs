<%- include('partials/header'); -%>

<h1>Home</h1>
<% if(contents.length < 1){ %>
  <p><%= homeStartingContent %></p>
<% } else { %>
  <% for(let i=contents.length-1; i>=0; i--){ %>
    <h3><%= contents[i].title %></h3>
    <p><%= contents[i].body %></p>
  <% } %>
<% } %>

<%- include('partials/footer'); -%>
